{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "barryvdh/laravel-cors": "^0.11.0",
        "bensampo/laravel-enum": "^1.25",
        "bugsnag/bugsnag-laravel": "^2.17",
        "cviebrock/eloquent-sluggable": "^6.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.3",
        "doctrine/dbal": "^2.9",
        "facade/ignition": "^1.9",
        "fideloper/proxy": "^4.0",
        "fzaninotto/faker": "^1.8",
        "laravel/framework": "^6.0",
        "laravel/passport": "^7.5",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "lorisleiva/laravel-deployer": "^0.2.12",
        "predis/predis": "^1.1",
        "rap2hpoutre/fast-excel": "^1.3.1",
        "renatomarinho/laravel-page-speed": "^1.8",
        "spatie/laravel-activitylog": "^3.8",
        "vatsim/sso-laravel": "3.1"
    },
    "require-dev": {
        "astrotomic/ignition-stackoverflow": "^1.4",
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-er-diagram-generator": "^1.3",
        "beyondcode/laravel-query-detector": "^1.1",
        "facade/ignition-code-editor": "^1.0",
        "facade/ignition-self-diagnosis": "^1.0",
        "facade/ignition-tinker-tab": "^1.0",
        "mazedlx/ignition-migrations": "^0.0.3",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "stefanbauer/ignition-laracasts-tab": "^0.2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": ["app/Helper.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
